task @PushToWild:
	Sh("ssh git.analit.net mkdir /var/www/git/${Configuration.Project}.git")
	Sh("ssh git.analit.net git init --bare /var/www/git/${Configuration.Project}.git")
	Sh("ssh git.analit.net ruby /var/www/git/install_hook.rb")
	Sh("git remote add origin ssh://git.analit.net/var/www/git/${Configuration.Project}.git")
	Sh("git push origin master")
